{
  "name": "soundworks-template",
  "authors": [
    "Benjamin Matuszewski",
    "SÃ©bastien Robaszkiewicz",
    "Norbert Schnell"
  ],
  "description": "soundworks application template.",
  "license": "BSD-3-Clause",
  "version": "2.2.0",
  "scripts": {
    "clear": "echo \"todo\"",
    "help": "node bin/runner --help",
    "minify": "for f in public/dist/js/*.min.js; do rm -f ${f}; done; for f in public/dist/js/*.js; do terser ${f} -o ${f//.js/.min.js}; done;",
    "postinstall": "mkdir -p public/dist/vendors && cp -R node_modules/@webcomponents/webcomponentsjs public/dist/vendors/ && cp -R node_modules/livereload public/dist/vendors/ && npm run transpile",
    "start": "node dist/server/index.js",
    "transpile": "rollup -c",
    "watch:transpile": "rollup -c --watch.chokidar",
    "watch:process": "node .bin/watch-process",
    "watch": "concurrently --names \"TRANSPILER,PROCESS\" \"npm run watch:transpile\" \"npm run watch:process server\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collective-soundworks/soundworks-template"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "lit-element": "^2.0.1",
    "livereload": "^0.8.0",
    "soundworks": "^2.2.1",
    "source-map-support": "^0.5.12",
    "waves-masters": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@ircam/jsdoc-template": "^1.0.2",
    "chokidar": "^3.0.0",
    "concurrently": "^4.1.0",
    "jsdoc": "^3.6.2",
    "np": "^5.0.2",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "terminate": "^2.1.2",
    "terser": "^4.0.0"
  }
}
