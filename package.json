{
  "name": "soundworks-template",
  "authors": [
    "Benjamin Matuszewski"
  ],
  "description": "soundworks application template",
  "license": "BSD-3-Clause",
  "version": "3.0.0-alpha.0",
  "scripts": {
    "dev:process": "soundworks-template-build --watch-process",
    "dev:process-inspect": "soundworks-template-build --watch-process-inspect",
    "dev:build": "soundworks-template-build --build --watch",
    "dev": "npm run build && (npm run dev:process-inspect server & npm run dev:build)",
    "build": "soundworks-template-build --build",
    "clean": "soundworks-template-build --clean",
    "minify": "soundworks-template-build --build && soundworks-template-build --minify",
    "prebuild": "npm run clean",
    "postinstall": "mkdir -p .vendors/public && cp -R node_modules/@webcomponents/webcomponentsjs .vendors/public",
    "start": "node dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collective-soundworks/soundworks-template"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@soundworks/core": "^3.0.0-alpha.1",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "json5": "^2.1.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "serve-static": "^1.14.1",
    "template-literal": "^1.0.3"
  },
  "devDependencies": {
    "@soundworks/template-build": "^3.0.0-alpha.1"
  }
}
