{
  "name": "soundworks-template",
  "authors": [
    "Benjamin Matuszewski"
  ],
  "description": "soundworks application template",
  "license": "BSD-3-Clause",
  "version": "2.2.0",
  "scripts": {
    "clean": "rm -Rf dist && rm -Rf public/dist/js && rm -Rf logs",
    "help": "node bin/runner --help",
    "minify": "for f in public/dist/js/*.min.js; do rm -f ${f}; done; for f in public/dist/js/*.js; do terser ${f} -o ${f//.js/.min.js}; done;",
    "start": "node dist/server/index.js",
    "transpile": "node .bin/build-application",
    "watch": "concurrently \"npm run watch:transpile\" \"npm run watch:process server\"",
    "watch:process": "node .bin/watch-process",
    "watch:transpile": "node .bin/build-application --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collective-soundworks/soundworks-template"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "lit-html": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@ircam/jsdoc-template": "^1.0.2",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "concurrently": "^4.1.0",
    "fs-extra": "^8.1.0",
    "jsdoc": "^3.6.2",
    "np": "^5.0.2",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "terminate": "^2.1.2",
    "terser": "^4.0.0"
  }
}
